<body>
  <table>
    <thead>
      <tr>
        <th>Server</th>
        <th>Online</th>
        <th>PVP</th>
        <th>XP Modifier</th>
        <th>Last Heartbeat</th>
      </tr>
    </thead>
    <tbody>
    <% @list.each do |item| %>
      <tr>
        <td><%= item[:server] %></td>
        <td><%= item[:online] %></td>
        <td><%= item[:pk_server] %></td>
        <td><%= item[:xp_modifier] %></td>
        <td><%= item[:updated_at] %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</body>